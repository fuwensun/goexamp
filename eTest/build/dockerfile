FROM ubuntu       
RUN apt-get update -y
RUN groupadd -r goms && useradd -r -g goms goms
USER goms
RUN ["pwd"]
WORKDIR /home/goms
COPY ./build/eTest ./              
COPY ./configs ./configs
RUN ["ls","-l","./eTest"]         
RUN ["ls","-l","./configs/"]         
CMD ["./eTest","-confpath=./configs"]