FROM ubuntu       
RUN apt-get update -y
RUN groupadd -r goms && useradd -r -g goms goms
USER goms
RUN ["pwd"]
WORKDIR /home/goms
COPY ./build/eMysql ./              
COPY ./configs ./configs
RUN ["ls","-l","./eMysql"]         
RUN ["ls","-l","./configs/"]         
CMD ["./eMysql","-cfgpath=./configs"]

