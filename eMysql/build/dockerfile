FROM ubuntu       
RUN apt-get update -y
RUN groupadd -r gMysql && useradd -r -g gMysql uMysql
USER uMysql
RUN ["pwd"]
WORKDIR /home/uMysql
COPY ./build/eMysql ./              
COPY ./configs ./configs
RUN ["ls","-l","./eMysql"]         
RUN ["ls","-l","./configs/"]         
CMD ["./eMysql","-confpath=./configs"]