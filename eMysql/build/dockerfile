FROM ubuntu:18.04       
LABEL maintainer "sfuwen@sina.com"   
RUN apt-get update -y
RUN groupadd -r goms && useradd -r -g goms goms
USER goms
WORKDIR /home/goms
COPY ./build/eMysql ./              
COPY ./configs ./configs
RUN ["ls","-l","./eMysql","./configs/"]         
EXPOSE 8080/tcp
EXPOSE 50051/tcp 
CMD ["./eMysql","-cfgpath=./configs"]

