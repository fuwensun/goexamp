FROM ubuntu:18.04       
LABEL maintainer="sfuwen@sina.com" 
RUN apt-get update -y
RUN groupadd -r goms && useradd -r -g goms goms
USER goms
RUN ["pwd"]
WORKDIR /home/goms
COPY ./build/eHttp ./              
RUN ["ls","-l","./eHttp"]         
EXPOSE 8080/tcp
CMD ["./eHttp","-s=string","-i=1","-b=true"]

