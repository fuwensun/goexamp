FROM ubuntu       
RUN apt-get update -y
RUN groupadd -r goms && useradd -r -g goms goms
USER goms
RUN ["pwd"]
WORKDIR /home/goms
COPY ./build/eConf ./              
COPY ./configs ./configs
RUN ["ls","-l","./eConf"]         
RUN ["ls","-l","./configs/"]         
CMD ["./eConf","-confpath=./configs"]