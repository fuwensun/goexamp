FROM ubuntu       
RUN apt-get update -y
RUN groupadd -r goms && useradd -r -g goms goms
USER goms
RUN ["pwd"]
WORKDIR /home/goms
COPY ./build/eRedis ./              
COPY ./configs ./configs
RUN ["ls","-l","./eRedis"]         
RUN ["ls","-l","./configs/"]         
CMD ["./eRedis","-cfgpath=./configs"]

