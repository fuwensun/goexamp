FROM ubuntu:18.04
LABEL maintainer="sfuwen@sina.com"
RUN apt-get update -y
# user
RUN ["whoami"]
RUN groupadd -r goms && useradd -r -g goms goms
USER goms
RUN ["whoami"]
# work
RUN ["pwd"]
WORKDIR /home/goms
RUN ["pwd"]
COPY ./build/eDocker ./eDocker
RUN ["ls","-l","./eDocker"]
# set
CMD ["./eDocker"]
